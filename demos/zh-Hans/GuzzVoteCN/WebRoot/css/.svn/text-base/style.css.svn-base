body, ul, ol, li, p, h1, h2, h3, h4, h5, h6, table, td, th, form, fieldset, img, dl, dt, dd {
	margin:0;
	padding:0;
	font-family: "宋体";
}
textarea, input, select, body {
	font-size:12px;
}
select, input {
	vertical-align:middle;
}
fieldset, img {
	border:0;
}
ul, ol {
	list-style:none;
}
img, div, td {
	behavior: url('/iepngfix.htc');
} 
a:link {
	color:#333;
	text-decoration:none;
}
a:visited {
	color:#333;
	text-decoration:none;
}
a:hover {
	color:#c00;
	text-decoration:underline;
	border:none;
}
h1, h2, h3, h4, h5, h6 {
	font-weight:normal;
}
.clearit {
	clear:both;
	font-size:0;
	line-height:0;
	height:0;
}
